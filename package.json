{
  "name": "@fastybird/smart-panel",
  "private": true,
  "version": "1.0.0-dev.1",
  "description": "Repository for FastyBird Smart Panel packages development",
  "keywords": [
    "fastybird",
    "fb",
    "ui",
    "smart-panel",
    "vue"
  ],
  "homepage": "https://smart-panel.fastybird.com",
  "bugs": "https://github.com/FastyBird/smart-panel/issues",
  "license": "Apache-2.0",
  "author": {
    "name": "FastyBird s.r.o.",
    "email": "code@fastybird.com",
    "url": "https://www.fastybird.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FastyBird/smart-panel.git"
  },
  "scripts": {
    "start": "pnpm run -r start",
    "start:dev": "pnpm run -r start:dev",
    "start:debug": "pnpm run -r start:debug",
    "start:prod": "pnpm run -r start:prod",
    "build": "pnpm run -r build",
    "clean": "pnpm run -r clean && rm -rf node_modules",
    "fix": "pnpm run -r fix",
    "graph": "nx graph",
    "lint:js": "pnpm run -r lint:js",
    "lint:js:fix": "pnpm run -r lint:js:fix",
    "lint:styles": "pnpm run -r lint:styles",
    "pretty": "pnpm run -r pretty:write && pnpm run -r pretty:check",
    "pretty:check": "pnpm run -r pretty:check",
    "pretty:write": "pnpm run -r pretty:write",
    "test:unit": "pnpm run -r test:unit",
    "test:watch": "pnpm run -r test:watch",
    "test:cov": "pnpm run -r test:cov",
    "test:debug": "pnpm run -r test:debug",
    "test:e2e": "pnpm run -r test:e2e",
    "generate:all": "pnpm run -r generate:spec && pnpm run -r generate:openapi",
    "generate:openapi": "pnpm --filter @fastybird/smart-panel-backend run generate:openapi && pnpm --filter @fastybird/smart-panel-admin run generate:openapi",
    "generate:spec": "pnpm run -r generate:spec",
    "bootstrap": "pnpm install && pnpm run generate:spec && pnpm --filter @fastybird/smart-panel-extension-sdk build && pnpm --filter @fastybird/smart-panel-extension-example build && pnpm run generate:openapi && pnpm --filter @fastybird/smart-panel-backend run typeorm:migration:run && pnpm --filter @fastybird/smart-panel-backend run build && pnpm --filter @fastybird/smart-panel-admin run build",
    "onboard": "pnpm --filter @fastybird/smart-panel-backend run cli:prod auth:onboarding",
    "admin:extensions": "pnpm --filter @fastybird/smart-panel-backend run cli:prod config:generate-admin-extensions",
    "admin:build": "pnpm --filter @fastybird/smart-panel-admin build",
    "admin:postinstall": "node -e \"process.exit(process.env.FB_NO_ADMIN_BUILD ? 0 : 1)\" || pnpm run admin:extensions && pnpm run admin:build"
  },
  "engines": {
    "node": ">=20",
    "pnpm": ">=10"
  },
  "packageManager": "pnpm@10.26.1",
  "pnpm": {
    "overrides": {
      "@types/node": "^20.17.0",
      "form-data": "^4.0.4",
      "axios": "^1.12.0",
      "tar-fs": "^3.0.9",
      "esbuild": "^0.25.5",
      "ip": "^2.0.1",
      "jose": "^6.0.11",
      "cookie": "^0.7.0",
      "cross-spawn": "^7.0.6",
      "nanoid": "^3.3.8",
      "path-to-regexp": "^8.2.0",
      "postcss": "^8.5.4",
      "on-headers": "^1.1.0",
      "tmp": "^0.2.4",
      "secp256k1": "^5.0.1",
      "multer": "^2.0.2",
      "validator": "^13.15.22",
      "jws": "^4.0.1",
      "@eslint/plugin-kit": "^0.3.4",
      "http-proxy-middleware": "^3.0.5",
      "debug": "^4.4.1",
      "undici": "^7.10.0",
      "braces": "^3.0.3",
      "micromatch": "^4.0.8",
      "mermaid": "^11.10.0",
      "js-yaml": "^4.1.1",
      "body-parser": "^2.2.1",
      "mdast-util-to-hast": "^13.2.1",
      "qs": "^6.14.1",
      "brace-expansion": "^2.0.2"
    },
    "onlyBuiltDependencies": [
      "@nestjs/core",
      "@parcel/watcher",
      "@swc/core",
      "@tailwindcss/oxide",
      "bcrypt",
      "esbuild",
      "sharp",
      "sqlite3",
      "vue-demi"
    ]
  }
}
