{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.vue",
    "src/**/*.json",
    "package.json",
    "../..//var/data/**/*.ts"
  ],
  "exclude": [
    "src/**/__tests__/*",
    "dist/**/*"
  ],
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "paths": {
      "@root-config/*": ["../..//var/data/*"]
    }
  }
}
