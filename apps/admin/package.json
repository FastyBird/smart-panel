{
  "name": "@fastybird/smart-panel-admin",
  "version": "1.0.0-dev.1",
  "type": "module",
  "description": "Powerful application for administrating and integrating the FastyBird IoT Smart Panel",
  "keywords": [
    "fastybird",
    "iot",
    "smart-panel",
    "admin",
    "administration",
    "iot-platform",
    "device-management",
    "home-automation",
    "api",
    "websockets",
    "iot-backend",
    "automation",
    "smart-home",
    "fastybird-platform",
    "smart-devices",
    "connected-devices",
    "integration"
  ],
  "homepage": "https://smart-panel.fastybird.com",
  "bugs": "https://github.com/FastyBird/smart-panel/issues",
  "license": "Apache-2.0",
  "author": {
    "name": "FastyBird s.r.o.",
    "email": "code@fastybird.com",
    "url": "https://www.fastybird.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FastyBird/smart-panel.git"
  },
  "files": [
    "dist/",
    "package.json",
    "README.md",
    "LICENSE.md"
  ],
  "scripts": {
    "build": "run-p type-check \"build:no-check {@}\" --",
    "build:no-check": "vite build",
    "start:dev": "vite",
    "start:preview": "vite preview",
    "start:prod": "serve -s ./dist",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "lint:styles": "stylelint 'src/**/*.scss'",
    "lint:deps": "npx madge --circular src --extensions ts,tsx --ts-config tsconfig.json",
    "pretty": "pnpm pretty:write && pnpm pretty:check",
    "pretty:check": "prettier \"src/**/*.{ts,scss,vue}\" --check",
    "pretty:write": "prettier \"src/**/*.{ts,scss,vue}\" --write",
    "test:unit": "vitest --run",
    "test:unit:watch": "vitest --watch",
    "test:cov": "vitest --run --coverage",
    "test:cov:watch": "vitest --watch --coverage",
    "test:e2e": "playwright test",
    "type-check": "vue-tsc --build",
    "generate:openapi": "openapi-typescript ../../spec/api/v1/openapi.json -o ./src/openapi.ts --enum --dedupe-enums --root-types --default-non-nullable=false",
    "generate:spec": "node ./tools/build_devices_spec.mjs && node ./tools/build_channel_spec.mjs && node ./tools/build_weather_cities.mjs"
  },
  "dependencies": {
    "@iconify/json": "^2.2.424",
    "@iconify/vue": "^4.3.0",
    "@sentry/vue": "^9.47.1",
    "@vue-leaflet/vue-leaflet": "^0.10.1",
    "@vueuse/core": "^12.8.2",
    "async-validator": "^4.2.5",
    "consola": "^3.4.2",
    "cron-parser": "^5.4.0",
    "date-fns": "^4.1.0",
    "dompurify": "^3.3.1",
    "element-plus": "^2.13.0",
    "gridstack": "^12.4.2",
    "i18n-iso-countries": "^7.14.0",
    "jwt-decode": "^4.0.0",
    "leaflet": "^1.9.4",
    "lodash": "^4.17.21",
    "marked": "^17.0.1",
    "md5": "^2.3.0",
    "mitt": "^3.0.1",
    "natural-orderby": "^5.0.0",
    "nprogress": "^0.2.0",
    "openapi-fetch": "^0.15.0",
    "openapi-typescript-helpers": "^0.0.15",
    "pinia": "^3.0.4",
    "serve": "^14.2.5",
    "socket.io-client": "^4.8.3",
    "unocss": "^66.5.12",
    "uuid": "^11.1.0",
    "vue": "^3.5.26",
    "vue-i18n": "^11.2.8",
    "vue-meta": "3.0.0-alpha.10",
    "vue-router": "^4.6.4",
    "vue3-cookies": "^1.0.6",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^6.0.8",
    "yaml": "^2.7.1",
    "@playwright/test": "^1.57.0",
    "@trivago/prettier-plugin-sort-imports": "^6.0.1",
    "@tsconfig/node22": "^22.0.5",
    "@types/dompurify": "^3.2.0",
    "@types/jsdom": "^21.1.7",
    "@types/lodash": "^4.17.21",
    "@types/md5": "^2.3.6",
    "@types/node": "^22.15.30",
    "@types/nprogress": "^0.2.3",
    "@unocss/transformer-variant-group": "^66.5.12",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/eslint-plugin": "^1.6.5",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-playwright": "^2.4.0",
    "eslint-plugin-vue": "^9.33.0",
    "jiti": "^2.6.1",
    "jsdom": "^26.1.0",
    "npm-run-all2": "^8.0.4",
    "openapi-typescript": "^7.10.1",
    "postcss": "^8.5.6",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.7.4",
    "sass": "^1.97.1",
    "sass-loader": "^16.0.6",
    "start-server-and-test": "^2.1.3",
    "stylelint": "^16.26.1",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-config-standard": "^37.0.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "stylelint-order": "^7.0.1",
    "stylelint-prettier": "^5.0.3",
    "typescript": "^5.8.3",
    "vite": "^6.4.1",
    "vite-plugin-vconsole": "^2.1.1",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vite-svg-loader": "^5.1.0",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.12"
  },
  "engines": {
    "node": ">=20",
    "npm": ">=10"
  }
}
