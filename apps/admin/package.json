{
  "name": "@fastybird/smart-panel-admin",
  "version": "1.0.0-dev.1",
  "type": "module",
  "description": "Powerful application for administrating and integrating the FastyBird IoT Smart Panel",
  "keywords": [
    "fastybird",
    "iot",
    "smart-panel",
    "admin",
    "administration",
    "iot-platform",
    "device-management",
    "home-automation",
    "api",
    "websockets",
    "iot-backend",
    "automation",
    "smart-home",
    "fastybird-platform",
    "smart-devices",
    "connected-devices",
    "integration"
  ],
  "homepage": "https://smart-panel.fastybird.com",
  "bugs": "https://github.com/FastyBird/smart-panel/issues",
  "license": "Apache-2.0",
  "author": {
    "name": "FastyBird s.r.o.",
    "email": "code@fastybird.com",
    "url": "https://www.fastybird.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FastyBird/smart-panel.git"
  },
  "scripts": {
    "build": "run-p type-check \"build:no-check {@}\" --",
    "build:no-check": "vite build",
    "start:dev": "vite",
    "start:preview": "vite preview",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "lint:styles": "stylelint 'src/**/*.scss'",
    "lint:deps": "npx madge --circular src --extensions ts,tsx --ts-config tsconfig.json",
    "pretty": "pnpm pretty:write && pnpm pretty:check",
    "pretty:check": "prettier \"src/**/*.{ts,scss,vue}\" --check",
    "pretty:write": "prettier \"src/**/*.{ts,scss,vue}\" --write",
    "test:unit": "vitest --run",
    "test:unit:watch": "vitest --watch",
    "test:cov": "vitest --run --coverage",
    "test:cov:watch": "vitest --watch --coverage",
    "test:e2e": "playwright test",
    "type-check": "vue-tsc --build",
    "generate:openapi": "openapi-typescript ../../spec/v1/openapi.json -o ./src/openapi.ts --enum --dedupe-enums --root-types --default-non-nullable=false"
  },
  "dependencies": {
    "@iconify/json": "^2.2.327",
    "@iconify/vue": "^4.3.0",
    "@sentry/vue": "^9.12.0",
    "@vueuse/core": "^12.8.2",
    "async-validator": "^4.2.5",
    "date-fns": "^4.1.0",
    "element-plus": "^2.9.7",
    "gridstack": "^12.0.0",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "mitt": "^3.0.1",
    "natural-orderby": "^5.0.0",
    "nprogress": "^0.2.0",
    "openapi-fetch": "^0.13.5",
    "openapi-typescript-helpers": "^0.0.15",
    "pinia": "^3.0.2",
    "unocss": "^66.0.0",
    "uuid": "^11.1.0",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.3",
    "vue-meta": "3.0.0-alpha.10",
    "vue-router": "^4.5.0",
    "vue-virtual-scroller": "2.0.0-beta.8",
    "vue3-cookies": "^1.0.6",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^6.0.5",
    "@playwright/test": "^1.51.1",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@tsconfig/node22": "^22.0.1",
    "@types/jsdom": "^21.1.7",
    "@types/lodash": "^4.17.16",
    "@types/md5": "^2.3.5",
    "@types/node": "^20.17.30",
    "@types/nprogress": "^0.2.3",
    "@unocss/transformer-variant-group": "^66.0.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitest/coverage-v8": "^3.1.1",
    "@vitest/eslint-plugin": "^1.1.42",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.24.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-playwright": "^2.2.0",
    "eslint-plugin-vue": "^9.33.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.1.0",
    "npm-run-all2": "^7.0.2",
    "openapi-typescript": "^7.6.1",
    "postcss": "^8.5.3",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.5.3",
    "sass": "^1.86.3",
    "sass-loader": "^16.0.5",
    "start-server-and-test": "^2.0.11",
    "stylelint": "^16.18.0",
    "stylelint-config-recommended-vue": "^1.6.0",
    "stylelint-config-standard": "^37.0.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^5.0.3",
    "typescript": "^5.8.3",
    "vite": "^6.2.6",
    "vite-plugin-vconsole": "^2.1.1",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vite-svg-loader": "^5.1.0",
    "vitest": "^3.1.1",
    "vue-tsc": "^2.2.8"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
