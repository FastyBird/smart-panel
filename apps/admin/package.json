{
  "name": "@fastybird/smart-panel-admin",
  "version": "1.0.0-dev.1",
  "type": "module",
  "description": "Powerful application for administrating and integrating the FastyBird IoT Smart Panel",
  "keywords": [
    "fastybird",
    "iot",
    "smart-panel",
    "admin",
    "administration",
    "iot-platform",
    "device-management",
    "home-automation",
    "api",
    "websockets",
    "iot-backend",
    "automation",
    "smart-home",
    "fastybird-platform",
    "smart-devices",
    "connected-devices",
    "integration"
  ],
  "homepage": "https://smart-panel.fastybird.com",
  "bugs": "https://github.com/FastyBird/smart-panel/issues",
  "license": "Apache-2.0",
  "author": {
    "name": "FastyBird s.r.o.",
    "email": "code@fastybird.com",
    "url": "https://www.fastybird.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FastyBird/smart-panel.git"
  },
  "scripts": {
    "build": "run-p type-check \"build:no-check {@}\" --",
    "build:no-check": "vite build",
    "start:dev": "vite",
    "start:preview": "vite preview",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "lint:styles": "stylelint 'src/**/*.scss'",
    "pretty": "pnpm pretty:write && pnpm pretty:check",
    "pretty:check": "prettier \"src/**/*.{ts,scss,vue}\" --check",
    "pretty:write": "prettier \"src/**/*.{ts,scss,vue}\" --write",
    "test:unit": "vitest --run",
    "test:unit:watch": "vitest --watch",
    "test:cov": "vitest --run --coverage",
    "test:cov:watch": "vitest --watch --coverage",
    "test:e2e": "playwright test",
    "type-check": "vue-tsc --build",
    "generate:openapi": "openapi-typescript ../../spec/v1/openapi.json -o ./src/openapi.ts --enum --dedupe-enums --root-types --default-non-nullable=false",
    "circular": "npx madge --circular src --extensions ts,tsx --ts-config tsconfig.json"
  },
  "dependencies": {
    "@iconify/vue": "^4.3",
    "@sentry/vue": "^9.3",
    "@vueuse/core": "^12.7",
    "async-validator": "^4.2",
    "date-fns": "^4.1",
    "element-plus": "^2.9",
    "jwt-decode": "^4.0",
    "lodash": "^4.17",
    "md5": "^2.3",
    "mitt": "^3.0",
    "natural-orderby": "^5.0",
    "nprogress": "^0.2",
    "openapi-fetch": "^0.13",
    "openapi-typescript-helpers": "^0.0.15",
    "pinia": "^3.0",
    "unocss": "^66.0",
    "uuid": "^11.1",
    "vue": "^3.5",
    "vue-i18n": "^11.1",
    "vue-meta": "^3.0.0-alpha.10",
    "vue-router": "^4.5",
    "vue3-cookies": "^1.0",
    "zod": "^3.24"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^6.0",
    "@playwright/test": "^1.50",
    "@trivago/prettier-plugin-sort-imports": "^5.2",
    "@tsconfig/node22": "^22.0",
    "@types/jsdom": "^21.1",
    "@types/lodash": "^4.17",
    "@types/md5": "^2.3",
    "@types/node": "^20.17",
    "@types/nprogress": "^0.2",
    "@unocss/transformer-variant-group": "^66.0",
    "@vitest/coverage-v8": "^3.0",
    "@vitejs/plugin-vue": "^5.2",
    "@vitest/eslint-plugin": "^1.1",
    "@vue/eslint-config-prettier": "^10.2",
    "@vue/eslint-config-typescript": "^14.4",
    "@vue/test-utils": "^2.4",
    "@vue/tsconfig": "^0.7",
    "eslint": "^9.21",
    "eslint-plugin-import": "^2.31",
    "eslint-plugin-playwright": "^2.2",
    "eslint-plugin-vue": "^9.32",
    "jiti": "^2.4",
    "jsdom": "^26.0",
    "npm-run-all2": "^7.0",
    "openapi-typescript": "^7.6",
    "postcss": "^8.5",
    "postcss-scss": "^4.0",
    "prettier": "^3.5",
    "sass": "^1.85",
    "sass-loader": "^16.0",
    "start-server-and-test": "^2.0",
    "stylelint": "^16.14",
    "stylelint-config-recommended-vue": "^1.6",
    "stylelint-config-standard": "^37.0",
    "stylelint-config-standard-scss": "^14.0",
    "stylelint-order": "^6.0",
    "stylelint-prettier": "^5.0",
    "typescript": "^5.7",
    "vite": "^6.2",
    "vite-plugin-vconsole": "^2.1",
    "vite-plugin-vue-devtools": "^7.7",
    "vite-svg-loader": "^5.1",
    "vitest": "^3.0",
    "vue-tsc": "^2.2"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
