{
  "name": "@fastybird/smart-panel-backend",
  "version": "1.0.0-dev.1",
  "description": "Powerful backend for administrating and integrating the FastyBird IoT Smart Panel",
  "keywords": [
    "fastybird",
    "iot",
    "smart-panel",
    "backend",
    "administration",
    "iot-platform",
    "device-management",
    "home-automation",
    "api",
    "websockets",
    "iot-backend",
    "automation",
    "smart-home",
    "fastybird-platform",
    "smart-devices",
    "connected-devices",
    "integration"
  ],
  "homepage": "https://smart-panel.fastybird.com",
  "bugs": "https://github.com/FastyBird/smart-panel/issues",
  "license": "Apache-2.0",
  "author": {
    "name": "FastyBird s.r.o.",
    "email": "code@fastybird.com",
    "url": "https://www.fastybird.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FastyBird/smart-panel.git"
  },
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint:js": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:js:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:openapi": "spectral lint ../../spec/v1/openapi.json --fail-severity warn",
    "lint:deps": "npx madge --circular src --extensions ts,tsx --ts-config tsconfig.json",
    "pretty": "pnpm pretty:write && pnpm pretty:check",
    "pretty:check": "prettier \"src/**/*.ts\" \"test/**/*.ts\" --check",
    "pretty:write": "prettier \"src/**/*.ts\" \"test/**/*.ts\" --write",
    "test:unit": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/jest/bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "type-check": "tsc --noEmit",
    "generate:openapi": "openapi-typescript ../../spec/v1/openapi.json -o ./src/openapi.d.ts --root-types --default-non-nullable=false --path-params-as-types=true",
    "cli": "nestjs-command"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.0.16",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.16",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/platform-express": "^11.0.16",
    "@nestjs/platform-socket.io": "^11.0.16",
    "@nestjs/schedule": "^5.0.1",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^11.0.16",
    "bcrypt": "^5.1.1",
    "cache-manager": "^6.4.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cron": "^3.5.0",
    "influx": "^5.10.0",
    "inquirer": "^12.5.2",
    "lodash.isundefined": "^3.0.1",
    "lodash.omitby": "^4.6.0",
    "nestjs-command": "^3.1.5",
    "openweathermap-ts": "^1.2.10",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "socket.io": "^4.8.1",
    "sqlite3": "^5.1.7",
    "systeminformation": "^5.25.11",
    "typeorm": "^0.3.22",
    "uuid": "^11.1.0",
    "yaml": "^2.7.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.24.0",
    "@nestjs/cli": "^11.0.6",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.0.16",
    "@stoplight/spectral-cli": "^6.14.3",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.11.20",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/lodash.isundefined": "^3.0.9",
    "@types/lodash.omitby": "^4.6.9",
    "@types/md5": "^2.3.5",
    "@types/node": "^22.14.1",
    "@types/node-fetch": "^2.6.12",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "globals": "^15.15.0",
    "jest": "^29.7.0",
    "openapi-typescript": "^7.6.1",
    "prettier": "^3.5.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.1",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.1"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
